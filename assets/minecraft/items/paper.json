{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/paper"
    },
    "cases": [
      {
        "when": "settings",
        "model": {
          "type": "model",
          "model": "item/custom/settings_paper"
        }
      },
      {
        "when": "cosmetic",
        "model": {
          "type": "model",
          "model": "item/custom/cosmetic"
        }
      },
      {
        "when": "red_cross",
        "model": {
          "type": "model",
          "model": "item/custom/red_cross"
        }
      },
      {
        "when": "green_check",
        "model": {
          "type": "model",
          "model": "item/custom/green_check"
        }
      }
    ]
  }
}
